# .env.example

# Environment (development | production)
NODE_ENV=development

# OpenAI API key for integration with the OpenAI service
OPENAI_API_KEY=your-openai-api-key-here

# Port number for the Next.js application (optional)
PORT=3000

# Public API URL (Next.js app public configuration)
NEXT_PUBLIC_API_URL=http://localhost:3000